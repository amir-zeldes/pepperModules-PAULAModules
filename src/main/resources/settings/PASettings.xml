<setMgr:settings xmlNs:setMgr="irgendwo">
	<!--
		Hier werden die moeglichen specific Analyzers fuer die Analyse eines
		PAULA-Korpus angegeben
	-->
	<setMgr:setting xml:id="SPEC_ANALYZERS">
		<analyzers>
			<!--
				Mit einer Collection koennen Analyzer zu Gruppen zusammengeschlossen
				werden; path gibt einen Ordner an, indem die Dateien liegen
			-->
			<collection path="importer.paula.paula10.analyzer.specificAnalyzer.">
				<!--
					name gibt einen abstrakten Namen fuer den Analyzer an; className
					gibt den tatsaechlichen Namen der Klassendatei an; priority gibt
					die Prioritaet an, in der der Analyzer analysiert (ein hoeherer
					Wert bedeutet eine wichtigere Prioritaet); order gibt die
					Reihenfolge an, mit der die von diesem Analyzer analysierte Datei
					spaeter importiert werden soll
				-->
				<!-- Klassifizierung: MetaStructData-->
				<analyzer name="MetaStructDataAnalyzer" className="MetaStructDataAnalyzer"
					priority="2.0" order="1.0" />
				<!-- Klassifizierung: MetaAnnoData-->
				<!-- muss wichtigere Prioritaet haben haben als der MetaDataAnalayzer-->
				<analyzer name="MetaAnnoDataAnalyzer" className="MetaAnnoDataAnalyzer"
					priority="2.0" order="1.0" />
				<!-- Klassifizierung: PrimData-->
				<analyzer name="PrimDataAnalyzer" className="PrimDataAnalyzer"
					priority="1.0" order="2.0" />
				<!-- muss wichtigere Prioritaet haben als StructDataAnalyser -->
				<!-- Klassifizierung: TokData-->
				<analyzer name="TokDataAnalyzer" className="TokDataAnalyzer"
					priority="2.0" order="2.0" />
				<!-- Klassifizierung: StructData-->
				<analyzer name="StructDataAnalyzer" className="StructDataAnalyzer"
					priority="1.0" order="2.0" />
				<analyzer name="RelDataAnalyzer" className="RelDataAnalyzer"
					priority="1.0" order="2.0" />
				<!--
					muss unwichtigere Prioritaet haben haben als der
					MetaStructDataAnalayzer
				-->
				<analyzer name="StructEdgeDataAnalyzer" className="StructEdgeDataAnalyzer"
					priority="1.0" order="2.0" />
				<!-- Klassifizierung: AnnoData-->
				<analyzer name="AnnoDataAnalyzer" className="AnnoDataAnalyzer"
					priority="1.0" order="2.0" />
				<!-- Klassifizierung: AudioData-->
				<!-- muss wichtigere Prioritaet haben haben als der AnnoDataAnalyzer-->
				<analyzer name="AudioDataAnalyzer" className="AudioDataAnalyzer"
					priority="2.0" order="2.0" />
				<!-- Klassifizierung: ComplexAnnoData-->
				<analyzer name="ComplexAnnoDataAnalyzer" className="ComplexAnnoDataAnalyzer"
					priority="2.0" order="2.0" />
				<!-- Klassifizierung: MultiFeatData-->
				<analyzer name="MultiFeatAnalyzer" className="MultiFeatAnalyzer"
					priority="2.0" order="2.0" />
				<!-- Klassifizierung: TypePosData-->
				<analyzer name="TypePosDataAnalyzer" className="TypePosDataAnalyzer"
					priority="2.0" order="1.0" />
				<!-- Klassifizierung: TypePosValueData-->
				<analyzer name="TypePosValueDataAnalyzer" className="TypePosValueDataAnalyzer"
					priority="2.0" order="1.0" />
				<!-- <analyzer name= ""	className= ""	priority= ""	order= ""/> -->
			</collection>
		</analyzers>
	</setMgr:setting>
</setMgr:settings>
